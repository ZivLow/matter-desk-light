{
    "configurations": [
        {
            "name": "Linux",
            "compilerPath": "${config:idf.toolsPath}/tools/xtensa-esp-elf/esp-13.2.0_20230928/xtensa-esp-elf/bin/xtensa-esp32-elf-gcc",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "cStandard": "gnu17",
            "cppStandard": "gnu++23",
            "includePath": [
                "${config:idf.espIdfPath}/components/**",
                "${config:idf.espIdfPathWin}/components/**",
                "${config:idf.espAdfPath}/components/**",
                "${config:idf.espAdfPathWin}/components/**",
                "${workspaceFolder}/main/include/**",
                "${workspaceFolder}/third_party/connectedhomeip/src/**",
                "${workspaceFolder}/third_party/connectedhomeip/config/esp32/components/chip/**",
                "${workspaceFolder}/third_party/connectedhomeip/examples/platform/esp32/**",
                "${workspaceFolder}/third_party/connectedhomeip/zzz_generated/app-common/app-common/zap-generated/**",
                "${workspaceFolder}/third_party/connectedhomeip/third_party/mbedtls/repo/**",
                "${workspaceFolder}/**"
            ],
            "browse": {
                "path": [
                    "${config:idf.espIdfPath}/components",
                    "${config:idf.espIdfPathWin}/components",
                    "${config:idf.espAdfPath}/components/**",
                    "${config:idf.espAdfPathWin}/components/**",
                    "${workspaceFolder}/third_party/connectedhomeip/src/**",
                    "${workspaceFolder}/third_party/connectedhomeip/config/esp32/components/chip/**",
                    "${workspaceFolder}/third_party/connectedhomeip/examples/platform/esp32/**",
                    "${workspaceFolder}/third_party/connectedhomeip/zzz_generated/app-common/app-common/zap-generated/**",
                    "${workspaceFolder}/third_party/connectedhomeip/third_party/mbedtls/repo/**",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": false
            }
        }
    ],
    "version": 4
}